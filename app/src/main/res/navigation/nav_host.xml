<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_host"
    app:startDestination="@+id/fragment_landing"
    >

    <fragment
        android:id="@+id/fragment_landing"
        android:name="com.nikhil.startertemplate.screens.landing.LandingFragment"
        android:label="Landing"
        tools:layout="@layout/fragment_landing"
        >
    </fragment>

    <action
        android:id="@+id/action_global_fragment_landing"
        app:destination="@id/fragment_landing"
        app:enterAnim="@anim/nav_default_enter_anim"
        app:exitAnim="@anim/nav_default_exit_anim"
        app:popEnterAnim="@anim/nav_default_pop_enter_anim"
        app:popExitAnim="@anim/nav_default_pop_exit_anim"
        app:popUpTo="@+id/nav_host"
        app:popUpToInclusive="true"
        />

</navigation>